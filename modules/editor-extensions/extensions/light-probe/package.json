                ]
            },
            "lightmap:light-probe-log": {
                "methods": [
                    "log",
                    "default.log"
                ]
            },
            "lightmap:light-probe-finished": {
                "methods": [
                    "finished",
                    "default.finished"
                ]
            },
            "lightmap:light-probe-end": {
                "methods": [
                    "end",
                    "default.end"
                ]
            },
            "lightmap:light-probe-progress": {
                "methods": [
                    "progress",
                    "default.progress"
                ]
            },
            "light-probe:clear": {
                "methods": [
                    "clear",
                    "default.clear"
                ]
            }
        }
    }
