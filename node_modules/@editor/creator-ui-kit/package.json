{
    "name": "@editor/creator-ui-kit",
    "version": "0.2.17-beta.12",
    "description": "Creator 内使用的 ui 组件。组件能够同时在 web 和 electron 环境下运行。",
    "main": "index.js",
    "scripts": {
        "build": "concurrently \"webpack --config ./build/webpack.build.production.config\" \"npm run build:types\"",
        "build:dev": "webpack --config ./build/webpack.build.development.config",
        "build:types": "rimraf ./temp && tsc -p ./tsconfig.types.json && npm run generate:types",
        "generate:types": "api-extractor run --local --verbose && api-extractor run --local --verbose -c ./api-extractor.browser.json",
        "lint-staged:js": "eslint --ext .ts .",
        "lint:fix": "eslint --fix --cache --ext .ts .",
        "pre-commit": "lint-staged",
        "start:dev": "concurrently \"webpack serve --config ./build/webpack.develop.config.js\" \"electron --inspect=5858 ./example/main.js\"",
        "test:electron": "npx playwright test --config=tests/config/electron.config.ts",
        "test:web": "npx playwright test --config=tests/config/default.config.ts --project=webkit",
        "test:electron-report-json": "PLAYWRIGHT_JSON_OUTPUT_NAME=./tests/report.json npx playwright test --config=tests/config/electron.config.ts --reporter=json"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/cocos-creator/creator-ui-kit.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/cocos-creator/creator-ui-kit/issues"
    },
    "homepage": "https://github.com/cocos-creator/creator-ui-kit#readme",
    "devDependencies": {
        "@babel/core": "^7.14.3",
        "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
        "@mdit-vue/plugin-component": "^0.12.0",
        "@mdit-vue/plugin-sfc": "^0.12.0",
        "@microsoft/api-extractor": "^7.34.4",
        "@playwright/test": "^1.29.1",
        "@types/chroma-js": "^2.1.4",
        "@types/lodash": "^4.14.191",
        "@types/markdown-it": "^12.2.3",
        "@types/node": "^14.18.37",
        "@types/sinon": "^10.0.13",
        "@typescript-eslint/eslint-plugin": "^5.48.0",
        "@typescript-eslint/parser": "^5.48.0",
        "concurrently": "^8.2.0",
        "css-loader": "^6.7.3",
        "csstype": "^3.1.1",
        "electron": "13.1.2",
        "eslint": "^8.31.0",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-jsdoc": "^48.2.3",
        "eslint-plugin-vue": "^9.15.1",
        "fork-ts-checker-webpack-plugin": "^7.2.14",
        "html-loader": "^2.1.2",
        "html-webpack-plugin": "^5.3.2",
        "husky": "^6.0.0",
        "less": "^4.1.1",
        "less-loader": "^9.0.0",
        "lint-staged": "^11.0.0",
        "mime": "^3.0.0",
        "node-loader": "^2.0.0",
        "postcss": "^8.4.1",
        "prettier": "^2.8.8",
        "proxy": "^1.0.2",
        "raw-loader": "^4.0.2",
        "rimraf": "^3.0.2",
        "sass": "^1.57.1",
        "sass-loader": "^13.2.0",
        "sinon": "^15.0.1",
        "socksv5": "0.0.6",
        "style-loader": "^3.3.0",
        "terser-webpack-plugin": "^5.3.6",
        "thread-loader": "^3.0.4",
        "ts-loader": "^9.4.2",
        "typescript": "^4.9.4",
        "vue": "^2.7.16",
        "vue-loader": "^15.10.1",
        "webpack": "^5.75.0",
        "webpack-cli": "^5.0.1",
        "webpack-dev-server": "^4.11.1",
        "webpack-merge": "^5.8.0",
        "ws": "^8.10.0"
    },
    "dependencies": {
        "@electron/remote": "^1.2.1",
        "chroma-js": "^2.1.2",
        "dom-align": "^1.12.4",
        "fs-extra": "^11.1.0",
        "highlight.js": "^11.1.0",
        "lit": "^2.8.0",
        "lodash": "^4.17.10",
        "markdown-it": "^13.0.1",
        "qrcode": "^1.4.4",
        "tslib": "^2.6.0"
    },
    "lint-staged": {
        "**/*.{js,ts}": "npm run lint-staged:js"
    }
}
